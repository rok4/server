{
    "logger": {
        "output": "rolling_file",
        "level": "error",
        "file_prefix": "/var/log/rok4",
        "file_period": 3600
    },
    "threads": 4,
    "port": ":9000",
    "backlog": 0,
    "api": true,
    "cache": {
        "size": 1000,
        "validity": 60
    },
    "configurations": {
        "services": "/etc/rok4/services.json",
        "layers": "/etc/rok4/layers",
        "styles": "file:///etc/rok4/styles",
        "tile_matrix_sets": "file:///etc/rok4/tilematrixsets"
    }
}
